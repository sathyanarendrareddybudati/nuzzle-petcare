<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /

    # Serve existing files/directories directly
    RewriteCond %{REQUEST_FILENAME} -f [OR]
    RewriteCond %{REQUEST_FILENAME} -d
    RewriteRule ^ - [L]

    # Front controller
    RewriteRule ^ index.php [L]
</IfModule>

Options -Indexes
